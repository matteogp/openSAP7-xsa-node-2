{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "/odata/v4/(.*)",
		"destination": "srv_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "/node(.*)",
		"destination": "srv_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsjs)",
		"destination": "xsjs_api"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "xsjs_api",
		"authenticationType": "xsuaa"
	}, {
		"source": "/(.*)",
		"localDir": "resources",
		"authenticationType": "xsuaa",
		"replace": {
			"pathSuffixes": [
				"index.html"
			],
			"services": {
				"sapui5_sb": {
					"tag": "ui5"
				}
			}
		}
	}]
}